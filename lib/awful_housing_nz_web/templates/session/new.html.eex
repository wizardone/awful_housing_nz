<h4>Login</h4>
<%= form_for @changeset, session_path(@conn, :create), fn f -> %>
  <label>
    <%= error_tag f, :email %>
    Email: <%= text_input f, :email %>
  </label>
  <br />
  <label>
    <%= error_tag f, :password %>
    Password: <%= password_input f, :password %>
  </label>
  <br />
  <%= submit "Submit" %>
<% end %>
